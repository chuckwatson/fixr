<div class="index-container">
  <div>
    <h1>Select Bike Shop</h1>
  </div>

<!-- Where map should go -->

<!-- CARDS FOR INDEX LIST -->
  <% @shops.each do |shop| %>
    <div class="shop-card">
      <% link_to shop_path(shop), class: "shop-link" do %>
        <div class="shop-card-image">
        </div>
        <div class="shop-card-text">
          <h5><%= shop.name %></h5>
          <p>Distance: 1.1 miles - CHANGE ONCE MAP DONE</p>
          <p>Rating: 4.9* - CHANGE ONCE REVIEWS DONE</p>
        </div>
      <% end %>
    </div>
  <% end %>
</div>
