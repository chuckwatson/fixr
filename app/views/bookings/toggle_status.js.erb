
<% if @booking.job_complete == true %>
    $(".booking-<%= @booking.id %> > a").html('Complete').removeClass('btn-warning').addClass('btn-success')
<% else %>
    $(".booking-<%= @booking.id %> > a").html('Mark as complete').removeClass('btn-success').addClass('btn-warning')
<% end %>
$("#booking-status-<%= @booking.id %>").html("<%= @booking.job_complete %>")
